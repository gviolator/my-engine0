{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/TypeScriptToLua/master/tsconfig-schema.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "types": [],
    "strict": true,
    "sourceMap": true,
    "outDir": "build",
  },

  "files": [
    "main.ts"
  ],

  

  "tstl": {
    "luaTarget": "5.4",
    "noImplicitSelf": true
  }
}